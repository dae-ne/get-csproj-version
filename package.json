{
  "name": "get-csproj-version",
  "type": "module",
  "scripts": {
    "build": "ncc build src/index.js -o dist",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix --ext .js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:fix"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@xmldom/xmldom": "^0.8.10",
    "xpath": "^0.0.33"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.46.0",
    "husky": "^4.3.8",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3"
  }
}
